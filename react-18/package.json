{
	"name": "@beyond-js/react-18-widgets",
	"version": "1.0.0",
	"description": "Create web projects as standalone microfrontends with beyondJS and React 18",
	"license": "MIT",
	"modules": {
		"path": "./"
	},
	"libraries": {
		"imports": [
			"@beyond-js/kernel",
			"@beyond-js/widgets"
		]
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"platform": "web",
				"ports": {
					"bundles": 9112
				},
				"ts": {
					"compiler": "tsc"
				}
			},
			{
				"name": "ssr",
				"platform": "ssr",
				"ports": {
					"bundles": 9113
				},
				"ts": {
					"compiler": "tsc"
				}
			}
		]
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"@beyond-js/widgets": "~0.1.3",
		"@beyond-js/kernel": "~0.1.8"
	},
	"devDependencies": {
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
	}
}
