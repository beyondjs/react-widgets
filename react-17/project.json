{
  "scope": "beyond-js",
  "name": "react-17-widgets",
  "version": "0.0.1",
  "license": "MIT",
  "modules": {
    "path": "./"
  },
  "libraries": {
    "imports": [
      "@beyond-js/kernel",
      "@beyond-js/widgets"
    ]
  },
  "deployment": {
    "distributions": [
      {
        "name": "npm",
        "maps": "external",
        "npm": {
